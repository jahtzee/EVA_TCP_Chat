      <nestedClassifier xmi:type="uml:Class" xmi:id="_s5ZI4JK1Ee2EVNitZyU9YQ" name="InputHandler" visibility="package">
        <interfaceRealization xmi:id="_s6nQ4JK1Ee2EVNitZyU9YQ" client="_s5ZI4JK1Ee2EVNitZyU9YQ" supplier="_s6euAZK1Ee2EVNitZyU9YQ" contract="_s6euAZK1Ee2EVNitZyU9YQ"/>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_s7FyA5K1Ee2EVNitZyU9YQ" name="run" specification="_s5ZI4ZK1Ee2EVNitZyU9YQ">
          <language>java</language>
          <body>try {&#xD;
				BufferedReader cliInput = new BufferedReader(new InputStreamReader(System.in));&#xD;
				while(!isDone &amp;&amp; !Thread.currentThread().isInterrupted()) {&#xD;
					String msg = cliInput.readLine();&#xD;
					if (msg.equals(&quot;:quit&quot;)) {&#xD;
						outputToServer.println(msg); // sends :quit&#xD;
						cliInput.close();&#xD;
						shutdownClient();&#xD;
					} else {&#xD;
						outputToServer.println(msg); //output is sent to the server&#xD;
					}&#xD;
				}	&#xD;
			} catch (IOException e) {&#xD;
				shutdownClient();&#xD;
			}&#xD;
		</body>
        </ownedBehavior>
        <ownedOperation xmi:id="_s5ZI4ZK1Ee2EVNitZyU9YQ" name="run" method="_s7FyA5K1Ee2EVNitZyU9YQ"/>
