        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_s7HnMJK1Ee2EVNitZyU9YQ" name="ConnectionHandler" specification="_s6qUMJK1Ee2EVNitZyU9YQ">
          <ownedParameter xmi:id="_s7HnMZK1Ee2EVNitZyU9YQ" name="client" type="_s6JW0ZK1Ee2EVNitZyU9YQ"/>
          <ownedParameter xmi:id="_s7HnMpK1Ee2EVNitZyU9YQ" type="_s5jg8JK1Ee2EVNitZyU9YQ" direction="return"/>
          <language>java</language>
          <body>this.client = client;&#xD;
