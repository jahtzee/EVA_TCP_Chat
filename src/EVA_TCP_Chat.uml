          </ownedParameter>
          <language>java</language>
          <body>/*&#xD;
			 * Output String s on Client CLI&#xD;
			 */&#xD;
			output.println(s);&#xD;
		</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_s7IOQ5K1Ee2EVNitZyU9YQ" name="printConnectedUsers" specification="_s5l9MpK1Ee2EVNitZyU9YQ">
          <language>java</language>
          <body>messageToClient(&quot;Currently connected:&quot;);&#xD;
			for (ConnectionHandler ch :Server.this.connections) {&#xD;
				if (ch.client.isConnected())&#xD;
					messageToClient(&quot;- &quot;+ch.nickname);&#xD;
			}&#xD;
